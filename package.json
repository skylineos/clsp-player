{
  "name": "@skylineos/clsp-player",
  "title": "Skyline CLSP Player",
  "version": "0.22.0",
  "description": "Skyline Technology Solutions' CLSP Video Player.  Stream video in near-real-time in modern browsers.",
  "keywords": [
    "html5",
    "clsp",
    "video",
    "player",
    "skyline"
  ],
  "license": "MIT",
  "repository": "https://github.com/skylineos/clsp-player.git",
